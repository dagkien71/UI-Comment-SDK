{
  "name": "ui-comment-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern SDK for embedding comment functionality directly on web UI elements",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/ui-comment-sdk.min.js",
    "dist/ui-comment-sdk.esm.min.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "clean:prod": "rm -f dist/index.js dist/index.esm.js dist/index.umd.js",
    "dev": "rollup -c -w",
    "serve": "serve demo",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build:prod && npm run clean:prod",
    "prepack": "npm run build:prod && npm run clean:prod"
  },
  "keywords": [
    "comment",
    "ui",
    "sdk",
    "annotation",
    "feedback",
    "collaboration",
    "review",
    "sidebar",
    "modal",
    "tester",
    "support test"
  ],
  "author": "UI Comment SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagkien71/UI-Comment-SDK.git"
  },
  "homepage": "https://github.com/dagkien71/UI-Comment-SDK#readme",
  "bugs": {
    "url": "https://github.com/dagkien71/UI-Comment-SDK/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-postcss": "^4.0.2",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
